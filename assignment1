public void borrowBook(String bookName) {
	for(Book book : book) {
		if (book.getTitle().equals(bookName)) {
			book.rented();
		}
	}
 }
public void returnBook(String bookName) {
	for(Book book : book) {
		if (book.getTitle().equals(bookName)) {
			book.returned();

		}
		}
}


