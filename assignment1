package booktester;


public class bktt {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
		Book example = new Book();
		System.out.println("Title (should be 1984): " + example.getTitle()); /**creates book in inventory */ 
		System.out.println("Borrowed? (should be false): " + example.isBorrowed());
		example.rented();
		System.out.println("Borrowed? (should be true): " +example.isBorrowed());
		example.returned();
		System.out.println("Borrowed? (should be false): " + example.isBorrowed());
		example.changeTitle("The Girl with the Dragon Tattoo): " + example.getTitle);
		System.out.println("Title (should be The Girl with the Dragon Tattoo): " + example.getTitle());/**creates another book in inventory*/
		System.out.println("Borrowed? (should be false): " + example.isBorrowed());
		example.rented();
		System.out.println("Borrowed? (should be true): " +example.isBorrowed());
		example.returned();
		System.out.println("Borrowed? (should be false): " + example.isBorrowed());

	}

	private static int title(String string) {
		// TODO Auto-generated method stub
		return 0;
	}

}

